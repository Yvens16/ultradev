{
  "name": "remix-fire-kit",
  "version": "0.3.14",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "test:remix": "cross-env NODE_ENV=test binode -- @remix-run/dev:remix dev",
    "firebase:emulators:start": "firebase emulators:start --project demo-makerkit --import ./firebase-seed",
    "firebase:emulators:export": "firebase emulators:export --project demo-makerkit ./firebase-seed",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -i ./app/styles/index.css -o app/styles/dist.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "stripe:listen": "docker run --rm -it --name=stripe -v ~/.config/stripe:/root/.config/stripe stripe/stripe-cli:latest listen --forward-to http://host.docker.internal:3000/resources/stripe/webhook",
    "stripe:mock-server": "docker run --rm -it -p 12111-12112:12111-12112 stripe/stripe-mock:latest",
    "killports": "sh scripts/kill-ports.sh",
    "test": "vitest",
    "test:e2e": "firebase emulators:exec --project demo-makerkit --import ./firebase-seed \"sh ./scripts/test.sh\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "printWidth": 80,
    "singleQuote": true
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-popover": "1.0.6",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@react-email/body": "^0.0.2",
    "@react-email/button": "^0.0.9",
    "@react-email/column": "^0.0.7",
    "@react-email/head": "^0.0.5",
    "@react-email/html": "^0.0.4",
    "@react-email/preview": "^0.0.6",
    "@react-email/render": "^0.0.7",
    "@react-email/section": "^0.0.9",
    "@react-email/text": "^0.0.5",
    "@remix-run/node": "^1.19.0",
    "@remix-run/react": "^1.19.0",
    "@remix-run/serve": "^1.19.0",
    "@remix-run/server-runtime": "^1.19.0",
    "@sentry/node": "^7.59.3",
    "@sentry/react": "^7.59.3",
    "@sentry/tracing": "^7.59.3",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "csrf": "^3.1.0",
    "cva": "npm:class-variance-authority@^0.7.0",
    "date-fns": "^2.30.0",
    "firebase": "9.23.0",
    "firebase-admin": "^11.10.1",
    "heroicons": "^2.0.18",
    "i18next": "^23.2.11",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-fs-backend": "^2.1.5",
    "i18next-http-backend": "^2.2.1",
    "isbot": "^3.6.13",
    "nodemailer": "^6.9.4",
    "pino": "^8.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.0.2",
    "react-top-loading-bar": "^2.3.1",
    "reactfire": "^4.2.3",
    "recharts": "^2.7.2",
    "remix-i18next": "^5.3.0",
    "stripe": "^12.14.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.0",
    "@remix-run/eslint-config": "^1.19.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.44.0",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/node": "^20.4.2",
    "@types/nodemailer": "^6.4.8",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.0",
    "autoprefixer": "^10.4.14",
    "binode": "^1.0.5",
    "c8": "^7.13.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.2",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "firebase-tools": "^12.4.5",
    "happy-dom": "^10.5.2",
    "msw": "^0.49.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.26",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.1.6",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=14"
  }
}
